{
  "name": "release-assist",
  "version": "1.0.1",
  "description": "Helps you releasing new versions of your node.js and frontend projects",
  "main": "index.js",
  "bin": "bin.js",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "fs-jetpack": "^0.10.5",
    "inquirer": "^2.0.0",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "lint-staged": "^7.2.0",
    "mocha": "^3.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.5"
  },
  "scripts": {
    "test": "mocha --recursive \"spec/**/*.spec.js\"",
    "prettier": "prettier --write \"./**/*.{js,json,md}\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szwacz/release-assist.git"
  },
  "keywords": [
    "release",
    "changelog"
  ],
  "author": "Jakub Szwacz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/szwacz/release-assist/issues"
  },
  "homepage": "https://github.com/szwacz/release-assist#readme"
}
